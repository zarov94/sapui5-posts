{
    "_version": "1.8.0",
    "sap.app": {
        "id": "sap.fiori.postsApp",
        "type": "application",
        "i18n": "i18n/i18n.properies",
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "applicationVersion": {
			"version": "1.0.0"
		}
    },
    "sap.ui":{
        "technology": "UI5",
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5":{
        "rootView": {
            "viewName": "sap.fiori.postsApp.view.App",
            "type": "XML",
            "async": true
        },
        "dependencies": {
            "minUI5Vesrsion": "1.54",
            "libs": {
                "sap.m": {}
            }
        },
        "models": {
            "i18n": {
                "type": "sap.ui.mode.resource:Resource",
                "settings": {
                    "bundleName": "sap.fiori.postsApp.i18n.i18n"
                }
            }
        }
    }
}